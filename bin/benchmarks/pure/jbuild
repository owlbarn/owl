(jbuild_version 1)

(executables (
  (names        (test map_eval))
  (public_names (pure_test pure_map_eval))
  (package     owl_benchmarks)
  (libraries  (
    benchmarks_common
  ))
  (flags (:standard -safe-string))
  (modes      (byte))
  (js_of_ocaml ((flags --pretty)))
))

(alias
  ((name install)
  (deps (test.bc.js map_eval.bc.js))))
