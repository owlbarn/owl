(jbuild_version 1)

(executables (
  (public_names (owl))
  (names        (owl_bin_zoo))
  (modules      (owl_bin_zoo))
  (package      owl-zoo)
  (libraries    (
    owl
    owl-zoo
  ))
  (flags (:standard -safe-string))
  (link_flags (-linkall))
  (modes      (byte))
))


(executables (
  (public_names (owlviz))
  (names        (owl_bin_viz))
  (modules      (owl_bin_viz))
  (package      owl-zoo)
  (libraries    (
    owl
  ))
  (flags (:standard -safe-string))
  (link_flags   (-linkall))
  (modes        (native))
))


(install (
  (section bin)
  (package owl-zoo)
  (files (
    (owl_upload_gist.sh as owl_upload_gist.sh)
    (owl_download_gist.sh as owl_download_gist.sh)
    (owl_list_gist.sh as owl_list_gist.sh)
  ))
))
