(jbuild_version 1)

(executable (
  (name        zoo)
  (public_name owl)
  (package     owl_zoo)
  (libraries  (
    owl
    owl_zoo
  ))
  (flags (:standard -safe-string))
  (link_flags (-linkall))
  (modes      (byte))
))

(install (
  (section bin)
  (package owl_zoo)
  (files (
    (owl_upload_gist.sh as owl_upload_gist.sh)
    (owl_download_gist.sh as owl_download_gist.sh)
    (owl_list_gist.sh as owl_list_gist.sh)
  ))
))
